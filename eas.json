{
  "cli": {
    "version": ">= 3.10.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true,
        "cocoapods": "1.14.3"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "environment": "preview",
      "env": {
        "ENVIRONMENT": "preview"
      },
      "ios": {
        "cocoapods": "1.14.3"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "ENVIRONMENT": "production"
      },
      "ios": {
        "image": "sdk-51"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}